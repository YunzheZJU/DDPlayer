{
  "name": "ddplayer",
  "version": "1.0.0",
  "title": "DDPlayer",
  "description": "DDPlayer is a frontend project of DDMusic.",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server",
    "build": "webpack --progress --hide-modules --env.production",
    "serve": "live-server --entry-file=dist/index.html ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YunzheZJU/DDPlayer.git"
  },
  "keywords": [
    "ddplayer",
    "vue.js",
    "vue",
    "player",
    "music",
    "html5"
  ],
  "author": "YunzheZJU",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YunzheZJU/DDPlayer/issues"
  },
  "homepage": "https://github.com/YunzheZJU/DDPlayer#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "^6.26.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.4.0",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "live-server": "^1.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss": "^7.0.2",
    "postcss-loader": "^3.0.0",
    "precss": "^3.1.2",
    "style-loader": "^0.22.1",
    "stylelint": "^9.5.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.3.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.1",
    "vue-hot-reload-api": "^2.3.0",
    "vue-loader": "^15.3.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
    "web-webpack-plugin": "^4.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "engines": {
    "node": ">=8.11.4",
    "npm": ">=5.6.0",
    "yarn": ">=1.7.0"
  },
  "browserslist": [
    "> 5% in CN",
    "last 3 Chrome versions",
    "last 3 Firefox versions",
    "Safari >= 10",
    "Explorer >= 8",
    "Edge >= 12",
    "iOS >= 10",
    "Android >= 6"
  ],
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-runtime",
      "syntax-dynamic-import"
    ],
    "comments": true
  },
  "postcss": {
    "plugins": {
      "precss": {}
    }
  },
  "stylelint": {
    "extends": "stylelint-config-recommended-scss",
    "rules": {
      "font-family-no-duplicate-names": null,
      "no-empty-source": null
    }
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "commonjs": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/essential"
    ],
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": 2015,
      "sourceType": "module",
      "allowImportExportEverywhere": true
    },
    "rules": {
      "indent": [
        "error",
        4
      ],
      "linebreak-style": [
        "error",
        "windows"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "no-undef": 0,
      "no-console": 0,
      "no-extra-parens": 1,
      "block-scoped-var": 1,
      "curly": 1,
      "eqeqeq": 1,
      "no-global-assign": 1,
      "no-implicit-globals": 1,
      "no-labels": 1,
      "no-multi-str": 1,
      "comma-spacing": 1,
      "comma-style": 1,
      "func-call-spacing": 1,
      "keyword-spacing": 1,
      "lines-around-comment": 1,
      "no-multiple-empty-lines": 1,
      "space-infix-ops": 1,
      "arrow-spacing": 1,
      "no-var": 1,
      "prefer-const": 1,
      "array-callback-return": 1,
      "dot-location": 1,
      "dot-notation": 1,
      "no-eval": 1,
      "no-extend-native": 1,
      "no-extra-label": 1,
      "space-before-blocks": 1,
      "space-in-parens": 1,
      "space-unary-ops": 1,
      "spaced-comment": 1,
      "arrow-body-style": 1,
      "no-restricted-imports": 1,
      "no-duplicate-imports": 1,
      "no-useless-computed-key": 1,
      "no-useless-rename": 1,
      "rest-spread-spacing": 1
    }
  }
}
